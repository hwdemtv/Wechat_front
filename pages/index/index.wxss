.article_div_list {
		margin: 0 8px;
		height: 40px;
		overflow: hidden;
		position: relative;
}
.article_div_list_li {
		display: block;
		width: 100%;
		height: 40px;
		line-height: 43px;
		overflow: hidden;
		white-space: nowrap;
		text-overflow: ellipsis;
		color: #494949;
